<section class="py-5 text-container">
  <div class="row py-lg-5">
    <div class="col-lg-6 col-md-6 mx-auto">
      <p>Search for Pets by Name:</p>
      <%= bootstrap_form_with url: "/applications/#{@application.id}", method: :get, local: true do |form| %>
        <%= form.text_field :search %>

        <%= form.submit "Find this pet!" %>
      <% end %>
    </div>
  </div>
</section>
